{
  "name": "TODO",
  "description": "TODO",
  "version": "0.0.13",
  "scripts": {
    "dev": "sapper dev",
    "build": "NODE_ENV=production sapper build --legacy",
    "export": "NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "deploy": "git push && ssh sapper 'cd httpdocs; npm run pull'",
    "pull": "git pull && npm i && npm run build && touch ./tmp/restart.txt",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "validate": "svelte-check",
    "playground": "ts-node-dev --respawn contrib/playground",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate && npm --no-git-tag-version version patch && git add package.json package-lock.json"
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      ">5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-useragent": "^1.0.15",
    "morgan": "^1.10.0",
    "sharp": "^0.26.0",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.6.2",
    "@types/sharp": "^0.25.1",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "rambdax": "6.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.16",
    "svelte-preprocess": "^4.0.12",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
