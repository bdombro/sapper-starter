  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "NODE_ENV=production sapper build --legacy",
    "export": "NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "validate": "svelte-check",
    "prettier": "prettier --write --plugin-search-dir=.  ./**/*.{html,svelte,ts,js,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{html,svelte,ts,js,json,css,yaml,yml,gql,graphql}": [
      "npm run prettier"
    ]
  },
  "prettier": {
    "semi": false
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      ">5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
